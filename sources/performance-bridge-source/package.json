{
  "name": "performance-bridge-source",
  "version": "0.1.23",
  "description": "Performance Bridge Airbyte source",
  "keywords": [
    "airbyte",
    "source",
    "faros"
  ],
  "homepage": "https://developer-docs.analytical.info/",
  "author": "Lambda Curry LLC",
  "license": "Apache-2.0",
  "files": [
    "lib/",
    "resources/"
  ],
  "engines": {
    "node": ">=14.5"
  },
  "main": "./lib",
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts' && npm run lint -- --fix",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prepare": "npm run build",
    "test": "jest --verbose --color",
    "test-cov": "jest --coverage --verbose --color",
    "watch": "tsc -b -w src test"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "cache-manager": "^3.4.4",
    "commander": "^8.2.0",
    "crypto": "^1.0.1",
    "faros-airbyte-cdk": "^0.1.23",
    "qs": "^6.10.1",
    "verror": "^1.10.0"
  },
  "jest": {
    "coverageDirectory": "out/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "testTimeout": 10000,
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    }
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.36",
    "@types/cache-manager": "^3.4.2",
    "@types/luxon": "^2.0.5",
    "luxon": "^2.0.2"
  }
}
