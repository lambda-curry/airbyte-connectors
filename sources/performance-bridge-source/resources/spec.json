{
  "documentationUrl": "https://restapi.qgenda.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "QGenda Spec",
    "type": "object",
    "required": [
      "email",
      "password"
    ],
    "additionalProperties": false,
    "properties": {
      "email": {
        "type": "string",
        "title": "QGenda Login Email",
        "examples": [
          "user@qgenda.com"
        ]
      },
      "password": {
        "type": "string",
        "title": "QGenda Login Password",
        "airbyte_secret": true
      }
    }
  }
}